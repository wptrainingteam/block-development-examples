{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"login": true,
	"landingPage": "/wp-admin/post.php?post=1&action=edit",
	"steps": [
		{
			"step": "installPlugin",
			"pluginZipFile": {
				"resource": "url",
				"url": "https://raw.githubusercontent.com/WordPress/block-development-examples/deploy/zips/slotfill-2fb190.zip"
			}
		},
		{
			"step": "runPHP",
			"code": "<?php /* SLOTFILL_2FB190_EXAMPLE_LOAD can set to load individual slotFillExamples at src/index.js */ require '/wordpress/wp-load.php'; define('SLOTFILL_2FB190_EXAMPLE_LOAD', ''; );"
		},
		{
			"step": "writeFile",
			"path": "/wordpress/wp-content/plugins/slotfill-2fb190/individual-example-add-on.php",
			"data": "<?php function set_individual_example() { wp_add_inline_script( 'slotfill-2fb190', 'window.slotFill2FB190ExampleLoad = SLOTFILL_2FB190_EXAMPLE_LOAD', 'before' ); }"
		},
		{
			"step": "activatePlugin",
			"pluginPath": "slotfill-2fb190/slotfill-2fb190.php"
		}
	]
}
